def project_name = 'l-was'

pipeline {
    agent {
        label 'master'
    }
    stages {
        stage('Environment Setup') {
            steps {
                script {
                    openshift.withCluster() {
                        openshift.withProject(project_name) {
                            echo "Create Project ${project_name}"
                        }
                    }
                }
            }
        }
    }
}
